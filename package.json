{
  "name": "Zlesshelper",
	"displayName": "Zlesshelper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:lesshelper.lthfromSelection"
	],
	"repository": {
    "type": "git",
    "url": "https://github.com/milawoai/vscode-less-helper.git"
  },
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "lesshelper.lthfromSelection",
            "title": "less to html by select"

		}],
		"menus": {
			"editor/context": [
				{
					"when": "editorLangId == typescript || editorLangId == javascript || editorLangId == less || editorLangId == css || editorLangId == vue",
					"command": "lesshelper.lthfromSelection",
					"title": "less to html by select"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"dependencies": {
		"copy-paste": "^1.3.0",
		"less": "^4.1.2"
	},
	"devDependencies": {
        "@types/vscode": "^1.62.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "@vscode/test-electron": "^1.6.2"
    }

}
